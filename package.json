{
  "name": "otpservice",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "clean": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "dev": "ts-node src/local.ts",
    "operation": "ts-node src/operation.ts",
    "build": "npm run clean && npm run generate && webpack",
    "start":"node dist/local.js",
    "generate": "",
    "package": "yarn build && sls package",
    "deploy": "npm run clean && npm run generate && webpack && SLS_DEBUG=true AWS_PROFILE=lambda sls deploy -s prod --region ap-southeast-1",
    "deploy:staging": "npm run clean && npm run generate && webpack && SLS_DEBUG=true AWS_PROFILE=lambda sls deploy -c serverless-staging.yaml -s prod --region ap-southeast-1"
  },
